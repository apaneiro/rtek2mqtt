{
	"name": "RTEK2MQTT",
	"version": "1.5.1",
	"slug": "rtek2mqtt",
	"homeassistant_api": "no",
	"hassio_api": true,
	"description": "Bridge between the RTEK Smart Home System and an MQTT server to get states and control the RTEK devices from Home Assistant",
	"url": "https://github.com/apaneiro/rtek2mqtt",
	"arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
	"options": {
		"rtekHost":null,
		"rtekPort":8080,
		"rtekUser":"admin",
		"rtekPassword":" ",
		"mqttBaseTopic":"rtek",
		"mqttHost":"<auto_detect>",
		"mqttPort":"<auto_detect>",
		"mqttUser":"<auto_detect>",
		"mqttPassword":"<auto_detect>",
		"cameraSecondsOn":15,
		"debug":1
	},
	"schema": {
		"rtekHost":"str",
		"rtekPort":"int",
		"rtekUser":"str",
		"rtekPassword":"password",
		"mqttBaseTopic":"str",
		"mqttHost":"str",
		"mqttPort":"str",
		"mqttUser":"str?",
		"mqttPassword":"password?",
		"cameraSecondsOn":"int",
		"debug":"int"
	},
	"map": ["homeassistant_config"],
	"services": ["mqtt:need"]
}